  "description": "An Ionic project",
  "cordova": {
    "plugins": {
      "cordova-plugin-firebase-dynamiclinks": {
        "APP_DOMAIN": "planrecetas.grupomenta.com",
        "PAGE_LINK_DOMAIN": "planrecetas.page.link",
