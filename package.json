    "cordova-support-android-plugin": "^1.0.1",
    "cordova-support-google-services": "^1.4.0",
    "core-js": "^2.5.4",
    "date-fns": "^2.11.1",
    "firebase": "^7.11.0",
    "ionic2-calendar": "0.5.1",
    "rxjs": "~6.5.4",
    "tslib": "^1.11.1",
    "validator": "latest",
    "zone.js": "~0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/architect": "~0.803.25",
