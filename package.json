{
  "name": "PlanRecetas",
  "version": "0.0.1",
  "author": "Edwin Salvador",
  "homepage": "https://grupomenta.com/",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build:ios:dev": "cp src/environments/google_services/GoogleService-Info.dev.plist GoogleService-Info.plist && cp src/environments/google_services/google-services.dev.json google-services.json && ionic cordova platform rm ios && ENV=dev ionic cordova platform add ios && ENV=dev ionic cordova build ios",
    "build:ios:prod": "cp src/environments/google_services/GoogleService-Info.prod.plist GoogleService-Info.plist && cp src/environments/google_services/google-services.prod.json google-services.json && ionic cordova platform rm ios && ENV=prod ionic cordova platform add ios && ENV=prod ionic cordova build ios --prod --release",
    "build:android:dev": "cp src/environments/google_services/GoogleService-Info.dev.plist GoogleService-Info.plist && cp src/environments/google_services/google-services.dev.json google-services.json && ionic cordova platform rm android && ENV=dev ionic cordova platform add android && ENV=dev ionic cordova build android",
    "build:android:prod": "cp src/environments/google_services/GoogleService-Info.prod.plist GoogleService-Info.plist && cp src/environments/google_services/google-services.prod.json google-services.json && ionic cordova platform rm android && ENV=prod ionic cordova platform add android && ENV=prod ionic cordova build android --prod --release",
    "deploy:functions:dev": "firebase use dev-plan-recetas && firebase functions:config:set c.env=dev && firebase deploy --only functions",
    "deploy:functions:prod": "firebase use plan-recetas && firebase functions:config:set c.env=prod && firebase deploy --only functions"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "~8.2.14",
    "@angular/compiler": "~8.2.14",
    "@angular/core": "~8.2.14",
    "@angular/fire": "^5.4.2",
    "@angular/forms": "~8.2.14",
    "@angular/platform-browser": "~8.2.14",
    "@angular/platform-browser-dynamic": "~8.2.14",
    "@angular/router": "~8.2.14",
    "@ionic-native/calendar": "^5.26.0",
    "@ionic-native/core": "^5.22.0",
    "@ionic-native/firebase-dynamic-links": "^5.22.0",
    "@ionic-native/firebase-x": "^5.22.0",
    "@ionic-native/splash-screen": "^5.22.0",
    "@ionic-native/status-bar": "^5.22.0",
    "@ionic/angular": "^5.2.1",
    "cordova-android": "^8.1.0",
    "cordova-plugin-androidx": "^1.0.2",
    "cordova-plugin-androidx-adapter": "^1.1.0",
    "cordova-plugin-calendar": "^5.1.5",
    "cordova-plugin-firebase-analytics": "^4.1.2",
